void main() {
    int nombre = readInt("Veuillez choisir un nombre pour afficher sa table");
    for (int i = 0; i <= 15; i++) {
        if (i < 10) {
            println(" " + i + " x " + nombre + " = " + i * nombre);
        } else {
            println(i + " x " + nombre + " = " + i * nombre);
        }
    }
}

void main() {
    int taille = readInteger("Quelle est votre taille ? (en cm)");

    if (taille <= 39) {
        print("Valeur trop petite pour une taille !");
    } else if (taille >= 271) {
        print("Valeur trop grande pour une taille !");
    } else {
        print("Vous faites " + taille + " cm");
    }
}
